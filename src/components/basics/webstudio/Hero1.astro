
<!-- Asymmetric Hero Section with Avant-garde Design Influence -->
<section class="relative min-h-screen overflow-hidden bg-gradient-to-br from-slate-900 via-indigo-950 to-slate-950">
   <!-- Geometric Background Elements -->
   <div class="absolute inset-0 opacity-20">
     <div class="absolute top-1/4 left-1/5 w-64 h-64 rounded-full bg-orange-500 blur-3xl"></div>
     <div class="absolute bottom-1/3 right-1/4 w-80 h-80 rounded-full bg-blue-600 blur-3xl"></div>
     <div class="absolute top-1/2 right-1/3 w-40 h-40 rounded-full bg-purple-700 blur-3xl"></div>
   </div>
   
   <!-- Constructivist-inspired grid lines -->
   <div class="absolute inset-0 opacity-10">
     <div class="absolute top-0 left-1/3 w-px h-full bg-white"></div>
     <div class="absolute top-0 left-2/3 w-px h-full bg-white"></div>
     <div class="absolute top-1/3 left-0 w-full h-px bg-white"></div>
     <div class="absolute top-2/3 left-0 w-full h-px bg-white"></div>
   </div>
 
   <!-- Main Content Container with Asymmetric Layout -->
   <div class="relative z-10 container mx-auto px-4 py-12 h-screen flex flex-col md:flex-row items-start">
     
     <!-- Left Column: Title and Abstract Element -->
     <div class="w-full md:w-5/12 pt-16 md:pt-32 pr-0 md:pr-8 relative">
       <!-- Avant-garde Typography for "lossless" -->
       <div class="mb-4 transform -rotate-6 relative left-2">
         <span class="inline-block font-mono text-sm tracking-widest text-cyan-400 border border-cyan-400 px-3 py-1 transform -skew-x-12">lossless</span>
       </div>
       
       <!-- Title with Dynamic Typography -->
       <h1 class="font-sans font-black text-5xl md:text-6xl text-white leading-tight mb-6">
         <span class="block transform hover:translate-x-2 transition-transform duration-300">Level Up</span>
         <span class="block text-transparent bg-clip-text bg-gradient-to-r from-blue-400 to-purple-500">the Whole</span>
         <span class="block transform hover:-translate-x-2 transition-transform duration-300">Organization</span>
       </h1>
       
       <!-- Subtitle with Collage-like Styling -->
       <div class="max-w-lg">
         <p class="font-serif text-lg text-blue-100 leading-relaxed mb-8 pl-4 border-l-2 border-orange-500">
           With the right <span class="text-orange-400 font-medium">tools</span>, <span class="text-cyan-300 font-medium">mindset</span>, <span class="text-purple-400 font-medium">processes</span>, and <span class="text-green-400 font-medium">culture</span>, any organization can upskill everyone for the era of AI
         </p>
       </div>
       
       <!-- Abstract Geometric Element -->
       <div class="hidden md:block absolute -bottom-16 -left-12 w-40 h-40 border-l-4 border-t-4 border-orange-500 opacity-70"></div>
     </div>
     
     <!-- Right Column: Triptych Visual Element -->
     <div class="w-full md:w-7/12 mt-12 md:mt-0 relative">
       <!-- Main Image - Larger -->
       <div class="relative z-20 transform md:translate-y-12 md:translate-x-8 shadow-2xl rounded-sm overflow-hidden border-4 border-indigo-800">
         <img 
           src="https://ai-images.wstd.work/dev/82ecd145da56063112d84248642a1b37f7ee5d1ce01771db7581993b31e08ded.png" 
           alt="Conceptual representation of AI transformation" 
           class="w-full object-cover"
         />
         <!-- Overlay with Bauhaus-inspired elements -->
         <div class="absolute inset-0 bg-gradient-to-t from-black/70 to-transparent flex items-end">
           <div class="p-6">
             <div class="flex items-center space-x-4 mb-3">
               <div class="w-8 h-8 rounded-full bg-blue-500"></div>
               <div class="w-8 h-8 rounded-sm bg-orange-500"></div>
               <div class="w-16 h-1 bg-white"></div>
             </div>
             <p class="font-mono text-xs text-white/80 uppercase tracking-wider">AI-Driven Transformation Framework</p>
           </div>
         </div>
       </div>
       
       <!-- Second Image - Vertical Accent -->
       <div class="hidden md:block absolute top-1/4 -right-12 z-10 w-64 shadow-2xl rounded-sm overflow-hidden transform rotate-6 border-4 border-orange-700">
         <img 
           src="https://ai-images.wstd.work/dev/75c58411d8ffe71f14423faec63580805a64b9e4155965671ce1b1c6c1fe531a.png" 
           alt="Abstract organizational transformation" 
           class="w-full object-cover"
         />
       </div>
       
       <!-- Kinetic-inspired Motion Elements -->
       <div class="absolute -bottom-12 left-1/4 w-40 h-1 bg-cyan-500 transform rotate-45 origin-left"></div>
       <div class="absolute -bottom-8 left-1/3 w-32 h-1 bg-purple-500 transform rotate-45 origin-left"></div>
       <div class="absolute -bottom-4 left-1/2 w-24 h-1 bg-blue-500 transform rotate-45 origin-left"></div>
     </div>
   </div>
   
   <!-- Dashboard-inspired Navigation Elements -->
   <div class="absolute bottom-8 left-0 w-full z-30">
     <div class="container mx-auto px-4">
       <div class="flex flex-wrap justify-center md:justify-between items-center">
         <div class="hidden md:flex items-center space-x-6">
           <div class="flex items-center space-x-2">
             <div class="w-3 h-3 rounded-full bg-green-500"></div>
             <span class="text-xs text-white/80 font-mono">System Active</span>
           </div>
           <div class="h-6 w-px bg-white/20"></div>
           <div class="font-mono text-xs text-white/60">EST. COMPLETION: 2024</div>
         </div>
         
         <div class="flex items-center space-x-4">
           <button class="px-5 py-2 bg-transparent border border-white/30 text-white font-medium rounded-sm hover:bg-white/10 transition-colors">
             Explore Process
           </button>
           <button class="px-5 py-2 bg-gradient-to-r from-blue-600 to-purple-600 text-white font-medium rounded-sm hover:from-blue-700 hover:to-purple-700 transition-colors">
             Start Transformation
           </button>
         </div>
       </div>
     </div>
   </div>
 </section>
 